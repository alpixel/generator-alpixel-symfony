<body bgcolor="#F0F0F0" leftmargin="0" marginwidth="0" topmargin="0" marginheight="0" offset="0">
    <center>
        <!-- main container table -->
        <table border="0" cellpadding="0" cellspacing="0" height="100%" width="100%" id="bodyTable">
            <tr>
                <td align="center" height="100%" valign="top" width="100%" id="bodyCell">


                <!--[if gte mso 9]>
                <table align="center" border="0" cellspacing="0" cellpadding="0" width="600">
                <tr>
                <td align="center" valign="top">
                <![endif]-->



                    <!-- COMPANY LOGO -->
                    <table border="0" cellpadding="0" cellspacing="0" width="100%" class="templateContainer">
                        <tr>
                            <td align="center" valign="top">
                                <table border="0" cellpadding="0" cellspacing="0" width="100%" bgcolor="#F0F0F0" style="background-color:#F0F0F0">
                                    <tr>
                                        <td valign="top" align="center" height="10" style="height:50px;width:100%" width="100%"></td>
                                    </tr>
                                    <tr>
                                        <td valign="top">
                                            <table style="background-color:#F0F0F0" width="100%" bgcolor="#F0F0F0" cellspacing="0" cellpadding="0" align="center">
                                                <tr>
                                                    <td valign="top" align="center" style="width:100%" width="100%">
                                                        <img src="https://www.alpixel.fr/sites/default/files/logo.png" alt="" width="200" height="100" style="max-width:200px; padding-bottom: 0; display: inline !important; vertical-align: bottom;" />
                                                    </td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top" align="center" height="10" style="height:50px;width:100%" width="100%"></td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>


                    <!-- BLOCK HEADER -->
                    <table border="0" cellpadding="0" cellspacing="0" width="100%" class="templateContainer">
                        <tr>
                            <td align="center" valign="top">

                                <!-- Content -->
                                <table border="0" cellpadding="0" cellspacing="0" width="100%" bgcolor="#FFFFFF" style="background-color:#000000">
                                    <tr>
                                        <td valign="top">
                                            <table style="background-color:#000000" width="100%" bgcolor="#FFFFFF" cellspacing="0" cellpadding="0" align="center">

                                                <!-- SPACE -->
                                                <tr>
                                                    <td width="10%" height="15px" style="height:15px;width:10%"></td>
                                                    <td width="80%" height="15px" style="height:15px;width:80%"></td>
                                                    <td width="10%" height="15px" style="height:15px;width:10%"></td>
                                                </tr>

                                                <tr>
                                                    <td width="10%"  style="width:10%"></td>
                                                    <td width="80%" valign="top" align="center" style="width:80%">
                                                        <font face="Arial, sans-serif" color="#FFFFFF" style="display:block;font-size:15px;text-align:left;line-height:25px;">
                                                            {% block header %}{% endblock %}
                                                        </font>
                                                    </td>
                                                    <td width="10%"  style="width:10%"></td>
                                                </tr>

                                                <!-- SPACE -->
                                                <tr>
                                                    <td width="10%" height="15px" style="height:15px;width:10%"></td>
                                                    <td width="80%" height="15px" style="height:15px;width:80%"></td>
                                                    <td width="10%" height="15px" style="height:15px;width:10%"></td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>



                    <!-- BLOCK BODY -->
                    <table border="0" cellpadding="0" cellspacing="0" width="100%" class="templateContainer">
                        <tr>
                            <td align="center" valign="top">

                                <!-- Content -->
                                <table border="0" cellpadding="0" cellspacing="0" width="100%" bgcolor="#FFFFFF" style="background-color:#FFFFFF">
                                    <tr>
                                        <td valign="top">
                                            <table style="background-color:#FFFFFF" width="100%" bgcolor="#FFFFFF" cellspacing="0" cellpadding="0" align="center">

                                                <!-- SPACE -->
                                                <tr>
                                                    <td width="10%" height="20px" style="height:20px;width:10%"></td>
                                                    <td width="80%" height="20px" style="height:20px;width:80%"></td>
                                                    <td width="10%" height="20px" style="height:20px;width:10%"></td>
                                                </tr>

                                                <tr>
                                                    <td width="10%" style="width:10%"></td>
                                                    <td width="80%" valign="top" align="center" style="width:80%">
                                                        <font face="Arial, sans-serif" color="#000000" style="display:block;font-size:12px;text-align:left;line-height:20px;">
                                                            {% block body %}{% endblock %}
                                                        </font>
                                                    </td>
                                                    <td width="10%" style="width:10%"></td>
                                                </tr>

                                                <!-- SPACE -->
                                                <tr>
                                                    <td width="10%" height="20px" style="height:20px;width:10%"></td>
                                                    <td width="80%" height="20px" style="height:20px;width:80%"></td>
                                                    <td width="10%" height="20px" style="height:20px;width:10%"></td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>



                    <!-- BLOCK FOOTER -->
                    <table border="0" cellpadding="0" cellspacing="0" width="100%" class="templateContainer">
                        <tr>
                            <td align="center" valign="top">
                                <!-- FOOTER -->
                                <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                    <tr>
                                        <td valign="top">

                                            <table style="background-color:#F0F0F0" width="100%" bgcolor="#F0F0F0" cellspacing="0" cellpadding="0" align="center">

                                                <!-- SPACE -->
                                                <tr>
                                                    <td valign="top" align="center" height="30" style="height:30px;width:100%" width="100%"></td>
                                                </tr>
                                                <tr>
                                                    <td valign="top" align="center">
                                                        <div style="padding-left:20px;padding-right:20px;" class="paddingLeft20 paddingRight20">
                                                            <div style="text-align:center;">
                                                                <img src="https://www.alpixel.fr/sites/default/files/logo.png" alt="" width="150" height="75" style="max-width:150px; padding-bottom: 0; display: inline !important; vertical-align: bottom;" />
                                                            </div>
                                                        </div>
                                                    </td>
                                                </tr>


                                                <!-- SPACE -->
                                                <tr>
                                                    <td valign="top" align="center" height="20" style="height:20px;width:100%" width="100%"></td>
                                                </tr>

                                                <tr>
                                                    <td valign="top" align="center">
                                                        <div style="padding-left:20px;padding-right:20px;" class="paddingLeft20 paddingRight20">
                                                            <font face="Arial, sans-serif" color="#000000" style="display:block;font-size:11px;text-align:center;line-height:18px;">
                                                                Rue Jean de la Fontaine, Bâtiment B - 73420 VIVIERS-DU-LAC<br />
                                                                Tél. 04 79 00 00 00 - Fax 04 79 00 00 00<br />
                                                                contact@exemple.fr<br />
                                                            </font>
                                                            <font face="Arial, sans-serif" color="#000000" style="display:block;font-size:13px;text-align:center;font-weight:700;line-height:18px;">
                                                                <strong><a href="{{base_url}}" style="color:#000000;text-decoration:none;">{{base_url}}</a></strong>
                                                            </font>
                                                        </div>
                                                    </td>
                                                </tr>

                                                <!-- SPACE -->
                                                <tr>
                                                    <td valign="top" align="center" height="20" style="height:20px;width:100%" width="100%"></td>
                                                </tr>
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                                <!-- /end table footer -->


                            </td>
                        </tr>
                    </table>
                    <!-- /end table 600px -->


                <!--[if gte mso 9]>
                </td>
                </tr>
                </table>
                <![endif]-->
                </td> <!-- /end main table cell -->
            </tr> <!-- /end main table row -->
        </table> <!-- /end main table table -->
    </center>
</body>
</html>
