<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width=device-width">
    <title></title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">


    <style type="text/css">
        #outlook a{
            padding:0;
        }
        body{
            width:100% !important;
            -webkit-text-size-adjust:none;
            -webkit-font-smoothing:antialiased;
            height:100%;
            margin:0;
            padding:0;
            font-size:14px;
            color:#2A2A2A;
            font-family:Arial, Helvetica, sans-serif;
        }
        body,#bodyTable{
            background-color:#F0F0F0;
        }
        body,#bodyTable,#bodyCell{
            height:100%;
            margin:0;
            padding:0;
            width:100%;
        }
        #bodyTable{
            table-layout:fixed;
        }
        #bodyCell{
            padding-top:0px;
            padding-right:10px;
            padding-bottom:0px;
            padding-left:10px;
        }
        .clear{
            display:block;
            clear:both;
            width:0px;
            height:0px;
        }
        .templateContainer{
            max-width:600px;
        }
        table{
            mso-table-lspace:0pt;
            mso-table-rspace:0pt;
            margin:0 auto;
        }
        img{
            -ms-interpolation-mode:bicubic;
            border:none;
            font-size:14px;
            font-weight:bold;
            height:auto;
            line-height:100%;
            outline:none;
            text-decoration:none;
            text-transform:capitalize;
            max-width:100%;
        }
        td{
            width:600px;
            max-width:600px;
        }
        p,a,li,td,blockquote{
            mso-line-height-rule:exactly;
        }
        a[href^=tel],a[href^=sms]{
            color:inherit;
            cursor:default;
            text-decoration:none;
        }
        p,a,li,td,body,table,blockquote{
            -ms-text-size-adjust:100%;
            -webkit-text-size-adjust:100%;
        }
        a[class=btn],a.btn{
            text-transform:uppercase;
            background-color:#b5121b;
            color:#ffffff;
            display:inline-block;
            font-family:sans-serif;
            font-size:24px;
            font-weight:bold;
            line-height:53px;
            text-align:center;
            text-decoration:none;
            min-width:200px;
            -webkit-text-size-adjust:none;
            padding:0 30px;
        }

        h1,h2,h3,h4,h5,h6{
            display:block;
            margin:0;
            padding:0;
        }
        h1{
            /*@editable*/color:#81979b !important;
            /*@editable*/font-family:Arial;
            /*@editable*/font-size:20px;
            /*@editable*/font-style:normal;
            /*@editable*/font-weight:400;
            /*@editable*/line-height:150%;
            /*@editable*/text-align:center;
        }
        h2{
            /*@editable*/color:#81979b !important;
            /*@editable*/font-family:Arial;
            /*@editable*/font-size:18px;
            /*@editable*/font-style:italic;
            /*@editable*/font-weight:normal;
            /*@editable*/line-height:150%;
            /*@editable*/text-align:center;
        }
        h3{
            /*@editable*/color:#81979b !important;
            /*@editable*/font-family:Arial;
            /*@editable*/font-size:24px;
            /*@editable*/font-style:normal;
            /*@editable*/font-weight:400;
            /*@editable*/line-height:150%;
            /*@editable*/text-align:center;
        }
        h4{
            /*@editable*/color:#81979b !important;
            /*@editable*/font-family:Arial;
            /*@editable*/font-size:16px;
            /*@editable*/font-style:normal;
            /*@editable*/font-weight:400;
            /*@editable*/line-height:150%;
            /*@editable*/text-align:center;
            text-transform:uppercase;
        }

        *[class=paddingTop0]{
            padding-top:0px !important;
        }
        *[class=paddingTop10]{
            padding-top:10px !important;
        }
        *[class=paddingTop20]{
            padding-top:20px !important;
        }
        *[class=paddingTop30]{
            padding-top:30px !important;
        }
        *[class=paddingTop40]{
            padding-top:40px !important;
        }
        *[class=paddingTop50]{
            padding-top:50px !important;
        }
        *[class=paddingLeft0]{
            padding-left:0px !important;
        }
        *[class=paddingLeft10]{
            padding-left:10px !important;
        }
        *[class=paddingLeft20]{
            padding-left:20px !important;
        }
        *[class=paddingLeft30]{
            padding-left:30px !important;
        }
        *[class=paddingLeft40]{
            padding-left:40px !important;
        }
        *[class=paddingLeft50]{
            padding-left:50px !important;
        }
        *[class=paddingRight0]{
            padding-right:0px !important;
        }
        *[class=paddingRight10]{
            padding-right:10px !important;
        }
        *[class=paddingRight20]{
            padding-right:20px !important;
        }
        *[class=paddingRight30]{
            padding-right:30px !important;
        }
        *[class=paddingRight40]{
            padding-right:40px !important;
        }
        *[class=paddingRight50]{
            padding-right:50px !important;
        }
        *[class=paddingBottom0]{
            padding-bottom:0px !important;
        }
        *[class=paddingBottom10]{
            padding-bottom:10px !important;
        }
        *[class=paddingBottom20]{
            padding-bottom:20px !important;
        }
        *[class=paddingBottom30]{
            padding-bottom:30px !important;
        }
        *[class=paddingBottom40]{
            padding-bottom:40px !important;
        }
        *[class=paddingBottom50]{
            padding-bottom:50px !important;
        }


        @media only screen and (max-width: 600px){
            body{
                width:100% !important;
                min-width:100% !important;
                -webkit-text-size-adjust:none !important;
            }

            .templateContainer{
                width:90% !important;
                max-width:90% !important;
                margin:0 auto !important;
            }

            a.btn,a[class=btn]{
                display:block!important;
                margin-bottom:10px!important;
                background-image:none!important;
                margin-right:0!important;
            }

            table{
                width:100% !important;
                max-width:100% !important;
            }

            img[class=responsive]{
                width:100% !important;
                max-width:100% !important;
            }

            #bodyCell{
                padding-top:0px;
                padding-right:0px;
                padding-bottom:0px;
                padding-left:0px;
            }

            h1{
                /*@editable*/font-size:15px;
            }

            h2{
                /*@editable*/font-size:13px;
            }

            h3{
                /*@editable*/font-size:11px;
            }

            td[class=columnsContainer]{
                display:block !important;
                max-width:600px !important;
                width:100% !important;
            }

            td[class=columnsContainerCell]{
                padding-left:20px !important;
                padding-right:20px !important;
                padding-bottom:15px !important;
            }
        }
    </style>
</head>

